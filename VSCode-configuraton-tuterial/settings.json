{
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "explorer.autoReveal": false,
    "C_Cpp.updateChannel": "Insiders",
    "http.proxySupport": "off",
    "code-runner.executorMap": {
        "c": "chcp 65001 ; cls ; cd $dir ; gcc $fileName -g -o $fileNameWithoutExt.exe ; if (Test-Path ./$fileNameWithoutExt.exe) { ./$fileNameWithoutExt.exe }",
        "cpp": "chcp 65001 ; cls ; cd $dir ; g++ $fileName -g -o $fileNameWithoutExt.exe ; if (Test-Path ./$fileNameWithoutExt.exe) { ./$fileNameWithoutExt.exe }",
    },
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "terminal.integrated.rendererType": "dom",
    "C_Cpp.default.cppStandard": "c++11",
    "C_Cpp.default.cStandard": "c99",
    "code-runner.ignoreSelection": true,
    "latex-workshop.intellisense.file.exclude": [
        "**/*.aux",
        "**/*.bbl",
        "**/*.bcf",
        "**/*.blg",
        "**/*.idx",
        "**/*.ind",
        "**/*.lof",
        "**/*.lot",
        "**/*.out",
        "**/*.toc",
        "**/*.acn",
        "**/*.acr",
        "**/*.alg",
        "**/*.glg",
        "**/*.glo",
        "**/*.gls",
        "**/*.ist",
        "**/*.fls",
        "**/*.log",
        "**/*.nav",
        "**/*.snm",
        "**/*.fdb_latexmk",
        "**/*.synctex.gz",
        "**/*.run.xml",
        "**/*.exe"
    ],
    "files.exclude": {
        "**/*.exe": true
    },
    "code-runner.runInTerminal": true,
    "terminal.integrated.inheritEnv": false,
    "tabnine.experimentalAutoImports": true,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: LLVM, IndentWidth: 4 }",
    "editor.foldingStrategy": "indentation",
    "code-runner.executorMapByGlob": {
        "pom.xml": "cd $dir && mvn clean package"
    },
    "code-runner.executorMapByFileExtension": {
        ".vb": "cd $dir && vbc /nologo $fileName && $dir$fileNameWithoutExt",
        ".vbs": "cscript //Nologo",
        ".scala": "scala",
        ".jl": "julia",
        ".cr": "crystal",
        ".ml": "ocaml",
        ".exs": "elixir",
        ".hx": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        ".rkt": "racket",
        ".scm": "csi -script",
        ".ahk": "autohotkey",
        ".au3": "autoit3",
        ".kt": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
        ".kts": "kotlinc -script",
        ".dart": "dart",
        ".pas": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        ".pp": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        ".d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        ".hs": "runhaskell",
        ".nim": "nim compile --verbosity:0 --hints:off --run",
        ".csproj": "dotnet run --project",
        ".fsproj": "dotnet run --project",
        ".lisp": "sbcl --script",
        ".kit": "kitc --run",
        ".v": "v run",
        ".vsh": "v run",
        ".sass": "sass --style expanded",
        ".cu": "cd $dir && nvcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
    }
}
